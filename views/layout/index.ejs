<div id="feed-area">
    <form>
        <div id="feed-time-area">
            <div id="input-feed-time"></div>
        </div>
        <div id="label-feed-volume"><span>0</span> MLs</div>
        <div>
            <input id="input-feed-volume" type="range" class="custom-range" min="0" max="100" value="85"/>
        </div>
        <div>
            <button id="button-feed-save" type="submit" class="btn btn-primary">Save</button>
        </div>
    </form>
</div>

<div id="history-area" class="d-flex">
    <div id="input-history-last-month"></div>
    <div id="input-history-current-month"></div>
</div>

</body>
</html>